<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context="com.example.nut.happyhealthy.Food_Type_1"
    android:background="#fcf2f2">


    <LinearLayout
        android:id="@+id/container_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include
            android:id="@+id/toolbar"
            layout="@layout/toolbar" />
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="สุขภาพ"
        android:id="@+id/textView24"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textSize="20dp"
        android:textStyle="bold"
        android:textColor="#ffffff" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/container_toolbar"
        android:gravity="center_horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="DD-MM-YYYY"
            android:id="@+id/chooseDate"
            android:textColor="#797979"
            android:textSize="20dp"
            android:textStyle="bold"
            android:layout_below="@+id/container_toolbar"
            android:layout_marginTop="15dp" />
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/container_toolbar"
        android:background="#d1b8ed"
        android:layout_marginTop="65dp">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/container_toolbar"
            android:layout_centerVertical="true"
            android:gravity="center_horizontal"
            android:layout_marginTop="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="เบาหวาน"
                android:id="@+id/textView34"
                android:textSize="20dp"
                android:layout_gravity="center_vertical"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/container_toolbar"
            android:layout_marginTop="4dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ค่าระดับน้ำตาลก่อนอาหาร"
                android:id="@+id/CostSugarBefore"
                android:layout_marginLeft="30dp"
                android:textColor="#ffffff" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="บอกระดับการแจ้งเตือน"
                android:id="@+id/LevelCostBefore"
                android:textColor="#fb2222"
                android:layout_gravity="center_vertical"
                android:textStyle="bold"
                android:layout_marginTop="0.5dp"
                android:layout_marginLeft="15dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/container_toolbar"
            android:layout_marginBottom="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ค่าระดับน้ำตาลหลังอาหาร"
                android:id="@+id/CostSugarAfter"
                android:layout_marginLeft="30dp"
                android:textColor="#ffffff" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="บอกระดับการแจ้งเตือน"
                android:id="@+id/LevelCostAfter"
                android:textColor="#fb2222"
                android:layout_gravity="center_vertical"
                android:textStyle="bold"
                android:layout_marginTop="0.5dp"
                android:layout_marginLeft="15dp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#b9e0f2"
        android:layout_below="@+id/container_toolbar"
        android:layout_marginTop="185dp">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/container_toolbar"
            android:layout_centerVertical="true"
            android:gravity="center_horizontal"
            android:layout_marginTop="10dp" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ไต"
                android:id="@+id/textView37"
                android:textSize="20dp"
                android:layout_gravity="center_vertical"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/container_toolbar" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ค่าการทำงานไต "
                android:id="@+id/CostGFR"
                android:layout_marginLeft="30dp"
                android:textColor="#ffffff" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="บอกระดับการแจ้งเตือน"
                android:id="@+id/LevelCostGFR"
                android:textColor="#fb2222"
                android:textStyle="bold"
                android:layout_marginTop="0.5dp"
                android:layout_marginLeft="75dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/container_toolbar"
            android:layout_alignParentRight="true"
            android:gravity="right"
            android:layout_marginRight="25dp"
            android:layout_marginBottom="10dp">

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#f6b7c0"
        android:layout_marginTop="360dp"
        android:layout_alignLeft="@+id/container_toolbar">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/container_toolbar"
            android:layout_centerVertical="true"
            android:gravity="center_horizontal"
            android:layout_marginTop="10dp" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ความดัน"
                android:id="@+id/textView41"
                android:textSize="20dp"
                android:layout_gravity="center_vertical"
                android:textColor="#ffffff"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/container_toolbar" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ค่าความดันโลหิต"
                android:id="@+id/Cost_PreTop"
                android:layout_marginLeft="30dp"
                android:textColor="#ffffff" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="บอกระดับการแจ้งเตือน"
                android:id="@+id/LevelPreTop"
                android:textColor="#fb2222"
                android:textStyle="bold"
                android:layout_marginTop="0.5dp"
                android:layout_marginLeft="75dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/container_toolbar" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ค่าความดันโลหิต"
                android:id="@+id/Cost_PreDown"
                android:layout_marginLeft="30dp"
                android:textColor="#ffffff" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="บอกระดับการแจ้งเตือน"
                android:id="@+id/LevelPreDown"
                android:textColor="#fb2222"
                android:textStyle="bold"
                android:layout_marginTop="0.5dp"
                android:layout_marginLeft="75dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/container_toolbar" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ค่าความดันโลหิต"
                android:id="@+id/Cost_Heart"
                android:layout_marginLeft="30dp"
                android:textColor="#ffffff" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="บอกระดับการแจ้งเตือน"
                android:id="@+id/LevelHaert"
                android:textColor="#fb2222"
                android:textStyle="bold"
                android:layout_marginTop="0.5dp"
                android:layout_marginLeft="75dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/container_toolbar"
            android:layout_alignParentRight="true"
            android:gravity="right"
            android:layout_marginRight="25dp"
            android:layout_marginBottom="10dp">

        </LinearLayout>

    </LinearLayout>


</RelativeLayout>
