<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nut.happyhealthy.DisplayKidney">


    <LinearLayout
        android:id="@+id/container_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include
            android:id="@+id/toolbar"
            layout="@layout/toolbar" />
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="165dp">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">

            <ImageView
                android:layout_width="95dp"
                android:layout_height="35dp"
                android:id="@+id/imageView19"
                android:layout_marginLeft="50dp"
                android:src="@drawable/calendi" />


            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/tv_K_Date"
                android:textSize="16dp"
                android:background="@drawable/my_box_dis"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp" />
        </LinearLayout>


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">

            <ImageView
                android:layout_width="280dp"
                android:layout_height="28dp"
                android:id="@+id/imageView13"
                android:onClick="ClickDisLevelsGFR"
                android:src="@drawable/txtgfr"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="4dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/tv_K_CostGFR"
                android:textSize="16dp"
                android:background="@drawable/my_box_dis"
                android:layout_marginLeft="90dp" />


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">

            <ImageView
                android:layout_width="280dp"
                android:layout_height="28dp"
                android:id="@+id/imageViewsugatbefroe"
                android:layout_marginLeft="20dp"
                android:src="@drawable/disgfrtext" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="210dp"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/tv_K_LevelCostGFR"
                android:textSize="16dp"
                android:background="@drawable/my_border_waring"
                android:layout_marginLeft="65dp" />
        </LinearLayout>


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/imageView6"
                android:src="@drawable/addbutton"
                android:nestedScrollingEnabled="false"
                android:onClick="ClickAddKidney"
                android:layout_weight="1"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="2dp" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/imageView22"
                android:src="@drawable/button"
                android:onClick="ClickHistoryKidney"
                android:layout_weight="1"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="2dp" />
        </LinearLayout>


    </LinearLayout>

</RelativeLayout>
