<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.nut.happyhealthy.DisplayKidney"
    android:background="@drawable/displaykidney">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="165dp">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">

            <ImageView
                android:layout_width="95dp"
                android:layout_height="35dp"
                android:id="@+id/imageView19"
                android:layout_marginLeft="50dp"
                android:src="@drawable/calendi" />


            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/tv_K_Date"
                android:textSize="16dp"
                android:background="@drawable/my_box_dis"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="20dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <ImageView
                android:layout_width="95dp"
                android:layout_height="35dp"
                android:id="@+id/imageViewtime"
                android:layout_marginLeft="53dp"
                android:src="@drawable/timedis" />

            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/tv_K_Time"
                android:textSize="16dp"
                android:background="@drawable/my_box_dis"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp" />
        </LinearLayout>


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">

            <ImageView
                android:layout_width="280dp"
                android:layout_height="28dp"
                android:id="@+id/imageView13"
                android:onClick="ClickDisLevelsGFR"
                android:src="@drawable/txtgfr"
                android:layout_marginLeft="6dp"
                android:layout_marginRight="4dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/tv_K_CostGFR"
                android:textSize="16dp"
                android:background="@drawable/my_box_dis"
                android:layout_marginLeft="70dp" />


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">

            <ImageView
                android:layout_width="280dp"
                android:layout_height="28dp"
                android:id="@+id/imageViewsugatbefroe"
                android:layout_marginLeft="20dp"
                android:src="@drawable/disgfrtext" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/tv_K_LevelCostGFR"
                android:textSize="16dp"
                android:background="@drawable/my_border_waring"
                android:layout_marginLeft="50dp" />
        </LinearLayout>


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/imageView6"
                android:src="@drawable/addbutton"
                android:nestedScrollingEnabled="false"
                android:onClick="ClickEditDataUser"
                android:layout_weight="1"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="2dp" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/imageView22"
                android:src="@drawable/button"
                android:onClick="ClickSaveDataUser"
                android:layout_weight="1"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="2dp" />
        </LinearLayout>


    </LinearLayout>

</RelativeLayout>
