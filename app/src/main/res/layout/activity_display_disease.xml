<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nut.happyhealthy.DisplayDisease"
    android:background="#e3e2e2">


    <LinearLayout
        android:id="@+id/container_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include
            android:id="@+id/toolbar"
            layout="@layout/toolbar" />
    </LinearLayout>

    <ImageView
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:id="@+id/imageView31"
        android:src="@drawable/listhistory"
        android:onClick="ClickHistoryDiabetes"
        android:layout_marginTop="10dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="15dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="เบาหวาน"
        android:id="@+id/textView20"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textSize="20dp"
        android:textStyle="bold"
        android:textColor="#ffffff" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:id="@+id/imageView32"
        android:src="@drawable/arowback"
        android:layout_marginTop="7dp"
        android:layout_marginLeft="3dp"
        android:onClick="ClickBackHomeDisDiabetes" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="330dp"
        android:layout_marginTop="110dp"
        android:orientation="vertical"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:background="@drawable/my_box_diabetes">

        <LinearLayout
            android:layout_width="280dp"
            android:layout_height="45dp"
            android:orientation="horizontal"
            android:layout_marginTop="10dp"
            android:layout_gravity="center_horizontal"
            android:gravity="center_horizontal">


            <TextView
                android:id="@+id/tv_D_Date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="DD-MM-YYYY  HH:MM"
                android:textColor="#a6a6a6"
                android:textSize="24dp"
                android:layout_gravity="center_vertical"
                android:textStyle="bold"
                android:gravity="center_horizontal" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageViewsugatbefroe"
                android:layout_width="fill_parent"
                android:layout_height="30dp"
                android:layout_marginLeft="15dp"
                android:src="@drawable/tstsugarbefore"
                android:layout_marginRight="15dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:orientation="horizontal"
            android:background="@drawable/my_box_dis"
            android:layout_gravity="center_horizontal">

            <TextView
                android:id="@+id/tv_D_CostSugarBefore"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:background="@drawable/my_box_dis"
                android:textColor="#a6a6a6"
                android:textSize="20dp"
                android:layout_gravity="center_vertical"
                android:layout_weight="5"
                android:textStyle="bold"
                android:gravity="center_horizontal" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView23"
                android:layout_width="fill_parent"
                android:layout_height="30dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/tstsugarbeforedis"
                android:layout_marginRight="15dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:orientation="horizontal"
            android:background="@drawable/my_border_waring"
            android:layout_gravity="center_horizontal">

            <TextView
                android:id="@+id/tv_D_LevelBefore"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:background="@drawable/my_border_waring"
                android:textColor="#a6a6a6"
                android:textSize="20dp"
                android:layout_gravity="center_vertical"
                android:layout_weight="5"
                android:textStyle="bold"
                android:gravity="center_horizontal" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="fill_parent"
                android:layout_height="30dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/tstsugarafter"
                android:layout_marginRight="15dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:orientation="horizontal"
            android:background="@drawable/my_box_dis"
            android:layout_gravity="center_horizontal">

            <TextView
                android:id="@+id/tv_D_CostSugarAfter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/my_box_dis"
                android:textColor="#a6a6a6"
                android:textSize="20dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="5dp"
                android:layout_weight="5"
                android:textStyle="bold"
                android:gravity="center_horizontal" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView24"
                android:layout_width="fill_parent"
                android:layout_height="30dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/tstsugarafterdis"
                android:layout_marginRight="15dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:orientation="horizontal"
            android:background="@drawable/my_border_waring"
            android:layout_gravity="center_horizontal">

            <TextView
                android:id="@+id/tv_D_LevelAfter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:background="@drawable/my_border_waring"
                android:textColor="#a6a6a6"
                android:textSize="20dp"
                android:layout_gravity="center_vertical"
                android:layout_weight="5"
                android:textStyle="bold"
                android:gravity="center_horizontal" />
        </LinearLayout>


    </LinearLayout>

</RelativeLayout>
