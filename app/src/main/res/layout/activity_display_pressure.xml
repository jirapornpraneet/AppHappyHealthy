<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.nut.happyhealthy.DisplayPressure"
    android:background="@drawable/displaypre">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="120dp">

            <ImageView
                android:layout_width="95dp"
                android:layout_height="35dp"
                android:id="@+id/imageView19"
                android:layout_marginLeft="50dp"
                android:src="@drawable/calendi" />

            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/tv_P_Date"
                android:textSize="16dp"
                android:autoText="false"
                android:background="@drawable/my_box_dis"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="20dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <ImageView
                android:layout_width="95dp"
                android:layout_height="35dp"
                android:id="@+id/imageViewtime"
                android:layout_marginLeft="53dp"
                android:src="@drawable/timedis" />

            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/tv_P_Time"
                android:textSize="16dp"
                android:autoText="false"
                android:background="@drawable/my_box_dis"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">

            <ImageView
                android:layout_width="200dp"
                android:layout_height="26dp"
                android:id="@+id/imageViewsugatafter"
                android:layout_marginLeft="20dp"
                android:src="@drawable/txtpretop" />

        </LinearLayout>


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/tv_P_CostPressureDown"
                android:textSize="16dp"
                android:autoText="false"
                android:background="@drawable/my_box_dis"
                android:layout_marginLeft="90dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp">

            <ImageView
                android:layout_width="200dp"
                android:layout_height="26dp"
                android:id="@+id/imageViewpreDown"
                android:layout_marginLeft="20dp"
                android:src="@drawable/txtpredown" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/tv_P_CostPressureTop"
                android:textSize="16dp"
                android:autoText="false"
                android:background="@drawable/my_box_dis"
                android:layout_marginLeft="90dp" />

        </LinearLayout>


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="200dp"
                android:layout_height="32dp"
                android:id="@+id/imageViewDown"
                android:layout_marginLeft="20dp"
                android:src="@drawable/dispretoptxt" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/tv_P_LevelDown"
                android:textSize="16dp"
                android:autoText="false"
                android:background="@drawable/my_border_waring"
                android:layout_marginLeft="70dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="200dp"
                android:layout_height="32dp"
                android:id="@+id/imagepreDown2"
                android:layout_marginLeft="20dp"
                android:src="@drawable/dispredowntxt" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/tv_P_LevelTop"
                android:textSize="16dp"
                android:autoText="false"
                android:background="@drawable/my_border_waring"
                android:layout_marginLeft="70dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/imageView6"
                android:src="@drawable/addbutton"
                android:nestedScrollingEnabled="false"
                android:onClick="ClickAddPre"
                android:layout_weight="1"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="2dp" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/imageView22"
                android:src="@drawable/button"
                android:onClick="ClickHistoryPre"
                android:layout_weight="1"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="2dp" />
        </LinearLayout>

    </LinearLayout>

</RelativeLayout>
